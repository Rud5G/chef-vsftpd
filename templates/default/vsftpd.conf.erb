<% if @config -%>
<%   @config.each do |key, value| -%>
<%     next if value.nil? -%>
<%     begin next if value.empty? rescue NoMethodError end -%>
<%= key %>=<%=
       case value
       when Array
         value.join(",")
       when TrueClass
         'YES'
       when FalseClass
         'NO'
       else
         value
       end %>
<%   end -%>
<% end -%>
